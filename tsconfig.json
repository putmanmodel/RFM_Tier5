{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "types/*": ["types/*"],
      "modules/*": ["modules/*"],
      "engine/*": ["engine/*"]
    }
  },
"include": ["src/**/*"],
"exclude": []
}
   
